@{
    Layout = null;
}

<!doctype html>
<html>
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta content="width=device-width, initial-scale=1.0, minimum-scale=0.5, maximum-scale=4.0, user-scalable=yes" name="viewport">
<title>Data Report of Ctrip Online</title>
<!-- path -->
@using Ctrip.Corp.Report.Utility;
@{
    string cssPath = (Enums.ResLangEnum.en.ToString().Equals(ViewBag.Lang, StringComparison.OrdinalIgnoreCase) ? "en" : "zh-cn");
    @this.Css(cssPath + "/global.css", this.Request.UserAgent.ToLower().Contains("ipad") ? cssPath + "/global_pad.css" : "")
}
<script type="text/javascript">
    //国际化静态资源数据
    var corpReport_Common = @Html.Raw(ViewBag.Common);
    var corpReport_HotelStar = @Html.Raw(ViewBag.Model["jsData"]);
</script>
</head>

<body>
<div class="clearfix header">
</div>
<div class="view_wrap"><div class="clearfix container">
<div class="sub_nav">
	<div class="sub_logo">
		<i class="icon i4">&times;</i>
	</div>
</div>
<div class="main_cnt clearfix">
	<div class="filter_bar">
        <div class="menu_ico"></div>
	</div>
	<div class="hp hp3">
		<div class="full_box">
			<div class="box3">
				<h3>@ViewBag.Model["lbl_CorpHotelsBreakdownbySta"]</h3>
				<div class="" id="htlAgrStarID" style="height:250px">
				</div>
			</div>
			<div class="box4"><div class="inner">
				<h3><em class="tit1">@ViewBag.Model["lbl_CorpHotels"]</em><em class="tit2">@ViewBag.Model["lbl_MemHotels"]</em></h3>
				<ul class="list1" id="htlArgNightID">
                    <li class="no_border">
						<span style=""></span>
						<em><i></i></em>
					</li>
				</ul>
				<ul class="list2">
					<li class="no_border">@ViewBag.Model["lbl_5star"]</li>
					<li>@ViewBag.Model["lbl_4star"]</li>
					<li>@ViewBag.Model["lbl_3star"]</li>
					<li>@ViewBag.Model["lbl_2star"]</li>
				</ul>
				<ul class="list3" id="htlMbrNightID">
                    <li class="no_border">
						<span style=""></span>
						<em><i></i></em>
					</li>
				</ul>
			</div></div>
			<div class="box5">
				<h3>@ViewBag.Model["lbl_MemHotelBreakdownbyStar"]</h3>
				<div class="" id="htlMbrStarID" style="height:250px">
				</div>
			</div>
		</div>
		<div class="box2">
			<h3>@ViewBag.Model["lbl_CorHotelVsMemHotel"]</h3>
			<div class="filter_option option1">
				<label>@ViewBag.Model["lbl_HotelStarRating"]</label>
				<span id="htlStarLvID" mark='0'>
					@ViewBag.Model["lbl_All"]
					<i class="icon i5"></i>
				</span>
                <div class="option_box hidepop" style="display:none" id="htlStarLvSelID">
                    <ul>
                    <li><a href="javascript:;" mark='0'>@ViewBag.Model["lbl_All"]</a></li>
                    <li><a href="javascript:;" mark='5'>@ViewBag.Model["lbl_5star"]</a></li>
                    <li><a href="javascript:;" mark='4'>@ViewBag.Model["lbl_4star"]</a></li>
                    <li><a href="javascript:;" mark='3'>@ViewBag.Model["lbl_3star"]</a></li>
                    <li><a href="javascript:;" mark='2'>@ViewBag.Model["lbl_2star"]</a></li>
                    </ul>
                </div>
			</div>
            <div class="data_table dt1 M-table type1" id="htlArgTabID"></div>
            <div class="data_table dt2 M-table type1" id="htlMbrTabID"></div>
		</div>
	</div>
</div>
</div><input type="hidden" id="page_id" value="109216"/></div>

<div class="footer">
</div>
<div class="no_data" id="noDataID" style="display:none"><img src="http://pic.ctrip.com/droco/img/loading.gif" alt=""></div>

@this.Script("fame/jquery-1.9.1.min.js","commpub/common.js", "commpub/header.js", "fame/droco.js")
<script type="text/javascript">
    var lanType = "@Html.Raw(ViewBag.Lang)";
	//头部选中条件
	var _headSelectInfo = CM.hashAny(window.location.hash);
	//配置文件信息
    var _cfgInfo = @Html.Raw(ViewBag.Config);			
	//头部搜索条件
	var _headInitInfo = {
	    masterAccountInfo: @Html.Raw(ViewBag.JsonAccountIDs),
        departmentInfo: @Html.Raw(ViewBag.JsonPartInfo),
        consumerAttrInfo: @Html.Raw(ViewBag.JsonConsumerAttributes),
        nowTime: "@Html.Raw(ViewBag.Date)"
    };
    _headInitInfo.nowTime = _headInitInfo.nowTime.replace(new RegExp('-', 'g'),'/');
</script>
@this.Script("fame/highcharts.js", "hotel/HtlStar.js", "commpub/pageID.js")
</body>
</html>